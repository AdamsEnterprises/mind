.point-upvote
  .material-icons
    position relative
    top 2px
    margin-left 10px
    margin-right 5px
    // TODO: Use $teal.lighten-4 here.
    color #b2dfdb

  &.upvote:hover .material-icons
    // TODO: Use $secondary-color here.
    color #26a69a

  &.remove-upvote .material-icons
    // TODO: Use $secondary-color here.
    color #26a69a

  &.upvote
  &.remove-upvote
    cursor pointer
